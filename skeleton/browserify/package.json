    "backbone": "1.1.2",
    "backbone.localstorage": "1.1.7",
    "body-parser": "1.2.0",
    "bootstrap": "3.1.1",
    "bower": "1.3.3",
    "browserify": "4.1.5",
    "browserify-swap": "0.2.1",
    "chai": "1.9.1",
    "express": "4.2.0",
    "grunt": "0.4.4",
    "grunt-browserify": "2.1.0",
    "grunt-cli": "0.1.13",
    "grunt-concurrent": "0.5.0",
    "grunt-contrib-clean": "0.5.0",
    "grunt-contrib-connect": "0.7.1",
    "grunt-contrib-copy": "0.5.0",
    "grunt-contrib-jshint": "0.9.2",
    "grunt-contrib-watch": "0.6.1",
    "grunt-karma": "0.8.3",
    "grunt-nodemon": "0.2.1",
    "handlebars": "1.3.0",
    "hbsfy": "1.3.2",
    "jasmine-node": "2.0.0-beta4",
    "jquery": "2.1.1",
    "json2": "0.4.0",
    "karma": "0.12.16",
    "karma-browserify": "0.2.0",
    "karma-chrome-launcher": "0.1.3",
    "karma-firefox-launcher": "0.1.3",
    "karma-ie-launcher": "0.1.3",
    "karma-jasmine": "0.2.2",
    "karma-mocha": "0.1.3",
    "karma-phantomjs-launcher": "0.1.2",
    "karma-safari-launcher": "0.1.1",
    "karma-spec-reporter": "0.0.12",
    "lodash": "2.4.1",
    "markdown": "0.5.0",
    "minifyify": "3.0.9",
    "minimatch": "0.2.14",
    "mocha": "1.19.0",
    "ps-tree": "0.0.3",
    "remapify": "0.1.6",
    "sinon": "1.10.0",
    "sinon-chai": "2.5.0",
    "sqlite3": "2.2.3"
  },
  "scripts": {
    "test": "grunt build:dev check",
    "shrink": "rm -f npm-shrinkwrap.json && npm shrinkwrap --dev"
  },
  "browser": {
    "underscore": "lodash/dist/lodash.underscore"
  },
  "browserify": {
    "transform": [
      "browserify-swap"
    ]
  },
  "browserify-swap": {
    "@packages": [
      "underscore",
      "backbone",
      "backbone.localStorage"
    ],
    "production": {
      "underscore.js": "lodash/dist/lodash.underscore"
    }
  }
}
